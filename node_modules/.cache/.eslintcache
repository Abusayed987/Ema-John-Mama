              <label htmlFor=\"email\">Email</label>\n                    <input type=\"email\" name=\"email\" id=\"\" required />\n                </div>\n                <div className=\"from-control\">\n                    <label htmlFor=\"password\">Password</label>\n                    <input type=\"password\" name=\"password\" id=\"\" required />\n                </div>\n                <p className='error-text'>{error}</p>\n                <p className='error-text'>{success}</p>\n                <button className='btn-submit' type='submit' value='login'>Login</button>\n                <p>New to Ema-john? <Link to='/signup'> Create New Account</Link></p>\n            </form>\n        </div>\n    );\n};\n\nexport default Login;","/Users/md.abusayed/projects/MileStone-08/Practice/Ema-John/src/components/About/About.js",[],[],"/Users/md.abusayed/projects/MileStone-08/Practice/Ema-John/src/components/Shipping/Shipping.js",[],[],"/Users/md.abusayed/projects/MileStone-08/Practice/Ema-John/src/components/routes/PrivateRoute.js",[],[],"/Users/md.abusayed/projects/MileStone-08/Practice/Ema-John/src/components/layout/Main.js",[],[],"/Users/md.abusayed/projects/MileStone-08/Practice/Ema-John/src/components/Shop/Shop.js",[],[],"/Users/md.abusayed/projects/MileStone-08/Practice/Ema-John/src/components/Signup/Signup.js",[],[],"/Users/md.abusayed/projects/MileStone-08/Practice/Ema-John/src/firebase/firebase.infig.js",[],[],"/Users/md.abusayed/projects/MileStone-08/Practice/Ema-John/src/utilities/fakedb.js",[],[],"/Users/md.abusayed/projects/MileStone-08/Practice/Ema-John/src/components/ReviewItem/ReviewItem.js",[],[],"/Users/md.abusayed/projects/MileStone-08/Practice/Ema-John/src/components/Cart/Cart.js",["108"],[],"import React from 'react';\nimport { Link } from 'react-router-dom';\nimport './Cart.css'\nconst Cart = ({ cart }) => {\n    // console.log(cart);\n    let total = 0;\n    let shipping = 0;\n    let quantity = 0;\n    for (const product of cart) {\n        quantity = quantity + product.quantity;\n        total = total + product.price * product.quantity\n        shipping = shipping + product.shipping;\n    }\n    const tax = +(total * 0.10).toFixed(2);\n    const grandTotal = total + shipping + tax;\n    return (\n        <div className='cart'>\n            <h3>Order Summary </h3>\n            <p>Selected Items :{quantity}</p>\n            <p>Total Price : ${total}</p>\n            <p>Total Shipping Charge: ${shipping}</p>\n            <p>tax: ${tax}</p>\n            <h4>Grand Total : ${grandTotal}</h4>\n        </div>\n    );\n};\n\nexport default Cart;","/Users/md.abusayed/projects/MileStone-08/Practice/Ema-John/src/components/Header/Header.js",[],[],"/Users/md.abusayed/projects/MileStone-08/Practice/Ema-John/src/components/Product/Product.js",[],[],{"ruleId":"109","severity":1,"message":"110","line":10,"column":13,"nodeType":"111","messageId":"112","endLine":10,"endColumn":21},{"ruleId":"109","severity":1,"message":"113","line":7,"column":19,"nodeType":"111","messageId":"112","endLine":7,"endColumn":27},{"ruleId":"109","severity":1,"message":"114","line":11,"column":11,"nodeType":"111","messageId":"112","endLine":11,"endColumn":15},{"ruleId":"109","severity":1,"message":"115","line":2,"column":10,"nodeType":"111","messageId":"112","endLine":2,"endColumn":14},"no-unused-vars","'products' is assigned a value but never used.","Identifier","unusedVar","'setError' is assigned a value but never used.","'form' is assigned a value but never used.","'Link' is defined but never used."]